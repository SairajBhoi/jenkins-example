node {
        stage ('clone') {
            git 'https://github.com/SairajBhoi/jenkins-example.git'
        
        }
    
        stage ('Compile Stage') {
                
                    bat 'mvn clean compile'
        }

        stage ('Testing Stage') {
                 
                    bat 'mvn test'
        }


        stage ('Deployment Stage') {
                    bat 'mvn install'
        }
    
}
